--++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
-- 64-bit Scrambler
--++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
library IEEE;
use IEEE.std_logic_1164.all;
use IEEE.std_logic_unsigned.all;

----Pragma translate_off
library unisim ;
use unisim.vcomponents.all ;
----Pragma translate_on

entity scrambler is port (				 
	clk 		: in  std_logic ;						-- Clock
	rst 		: in  std_logic ;						-- Reset
	init 		: in  std_logic ;						-- Initialises scrambler
	addressin	: in  std_logic_vector(9  downto 0) ;   -- Endereco da porta A
	sain		: in  std_logic_vector(63 downto 0) ;	-- Scram da memoria - Porta A
	sbin		: in  std_logic_vector(63 downto 0) ;	-- Scram da memoria - Porta B
	word64in	: in  std_logic_vector(63 downto 0) ;	-- Input  data
	word64out	: out std_logic_vector(63 downto 0)) ;	-- Output data
end scrambler;

architecture scrambler of scrambler is
	signal s	 	 : std_logic_vector(63 downto 0) ;
	signal sa	 	 : std_logic_vector(63 downto 0) ;
	signal sb	 	 : std_logic_vector(63 downto 0) ;
	signal result	 : std_logic_vector(63 downto 0) ;
	signal resultfas : std_logic_vector(63 downto 0) ;
	signal address	 : std_logic_vector(9  downto 0) ;
	signal wordin	 : std_logic_vector(63 downto 0) ;
	signal wordout	 : std_logic_vector(63 downto 0) ;
	signal initscram : std_logic;
	signal repete	 : std_logic;
begin
	wordin    <= word64in;
	word64out <= wordout;
	initscram <= init;
	sa	  	  <= sain;
	sb	  	  <= sbin;
	
	-- Embaralha primeira palavra com o FAS
	resultfas <= wordin xor x"000000000000FFFF";

	-- Embaralha o resto
	result <= wordin xor s;
	
	process (clk, rst, initscram, address, repete, wordin)
	begin
		if clk'event and clk = '1' then
			if	rst = '1' then
				wordout <= wordin ;
				address <= (others => '0' ) ;
			else
				address   <= addressin;
				if initscram = '0' then
					wordout <= resultfas ;
					s <= sa;
				else
					if address = "1111111111" then
						s <= sa;
					else
						if repete = '0' then
							s <= sa;
						else
							s(63 downto 1) <= sa(62 downto 0);
							s(0) <= sb(63);	
						end if;				
					end if;				
					wordout <= result ;
				end if;
			end if;
		end if;
		
	end process;
	
	process (clk, rst, address, initscram)
	begin
		if rst = '1' then
			repete  <= '0';
		elsif clk'event and clk = '1' then
			if initscram = '0' then
				repete <= '0';
			else
				if address = "1111111111" then
					repete <= '1';
				end if;				
			end if;
		end if ;
	end process;

end scrambler ;

